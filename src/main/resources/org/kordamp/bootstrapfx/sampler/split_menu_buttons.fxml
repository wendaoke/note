    <SplitMenuButton GridPane.columnIndex="5" GridPane.rowIndex="0" styleClass="split-menu-btn, split-menu-btn-lg, split-menu-btn-danger" text="Danger"/>

    <SplitMenuButton GridPane.columnIndex="0" GridPane.rowIndex="1" styleClass="split-menu-btn, split-menu-btn-default" text="Default"/>
    <SplitMenuButton GridPane.columnIndex="1" GridPane.rowIndex="1" styleClass="split-menu-btn, split-menu-btn-primary" text="Primary"/>
