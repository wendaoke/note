    <SplitMenuButton GridPane.columnIndex="5" GridPane.rowIndex="1" styleClass="split-menu-btn, split-menu-btn-danger" text="Danger"/>

    <SplitMenuButton GridPane.columnIndex="0" GridPane.rowIndex="2" styleClass="split-menu-btn, split-menu-btn-sm, split-menu-btn-default" text="Default"/>
