    <SplitMenuButton GridPane.columnIndex="0" GridPane.rowIndex="3" styleClass="split-menu-btn, split-menu-btn-xs, split-menu-btn-default" text="Default"/>
    <SplitMenuButton GridPane.columnIndex="1" GridPane.rowIndex="3" styleClass="split-menu-btn, split-menu-btn-xs, split-menu-btn-primary" text="Primary"/>
    <SplitMenuButton GridPane.columnIndex="2" GridPane.rowIndex="3" styleClass="split-menu-btn, split-menu-btn-xs, split-menu-btn-success" text="Success"/>
